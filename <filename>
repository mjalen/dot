# Notes

# When combining all the config to one file, use:


# [[file:Config.org::*Notes][Notes:1]]
let
  concatAttr = list: foldl' (a: b: a // b) {} list;
in

concatAttr [
  
  # block 1
  (let
    # ...
  in {

  })

  # block 2
  (let
    # ...
  in {

  })

  # etc...
  # Each block must return an attrset.
]
# Notes:1 ends here
